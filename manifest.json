{
  "name": "JSONBeautifier",
  "description": "Beautify JSON with the click of a button.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "64": "/images/icon64.png",
      "128": "/images/icon128.png"
    }
  },
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "64": "/images/icon64.png",
    "128": "/images/icon128.png"
  }
}
